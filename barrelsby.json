  "directory": ["./libs", "./tools"],
  "delete": true,
  "location": "branch",
  "name": "index",
  "structure": "flat",
  "verbose": false,
  "singleQuotes": true,
  "include": [
    ".ts$",
    ".tsx$",
    ".js$",
    ".jsx$",
    ".md$",
    ".mdx$",
    ".svg$",
